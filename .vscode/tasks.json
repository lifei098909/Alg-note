{
    "version": "2.0.0",
    "command": "gcc",
    "args": [
        "-g",
        "${workspaceFolder}\\main.c",
        "${workspaceFolder}\\list.c",
        "${workspaceFolder}\\array.c",
        "${workspaceFolder}\\common.c",
        "${workspaceFolder}\\dynamic_programming.c",
        "-o",
        "${fileBasenameNoExtension}.exe"],    // 编译命令参数
    "problemMatcher": {
        "owner": "cpp",
        "fileLocation": ["relative", "${workspaceFolder}"],
        "pattern": {
            "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
        }
    }
}